{
	"version": "0.0.1",
	"name": "radicle-vscode-extension",
	"displayName": "radicle-vscode-extension",
	"description": "Radicle extension for VS Code",
	"keywords": [
		"radicle",
		"vscode",
		"extension",
		"maninak"
	],
	"publisher": "Radicle IDE Plugins Team",
	"author": "Konstantinos Maninakis <konstantinos.maninakis@sclable.com>",
	"contributors": [
		"Konstantinos Maninakis <konstantinos.maninakis@sclable.com>"
	],
	"license": "Apache-2.0",
	"repository": "https://github.com/cytechmobile/radicle-vscode-extension",
	"bugs": "https://github.com/cytechmobile/radicle-vscode-extension/issues?q=is%3Aissue+is%3Aopen+sort%3Aupdated-desc",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"scripts": {
		"preinstall": "npx -y only-allow pnpm",
		"vscode:prepublish": "npm run build && npm run test && npm run lint",
		"verify-deps": "npx pnpm i --frozen-lockfile --prefer-offline",
		"build": "npm run verify-deps && tsc",
		"dev": "npm run verify-deps && tsc -watch",
		"test": "echo \"Testing not yet implemented.\"",
		"lint": "eslint \"src/**/*.ts\""
	},
	"main": "./dist/extension.js",
	"activationEvents": [],
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"eslint": "^8.33.0",
		"typescript": "^4.9.4"
	},
	"private": true
}
